{
  "name": "react-clustergrammer",
  "version": "1.1.13",
  "description": "React wrapper for clustergrammer",
  "homepage": "https://hobincar.github.io/react-clustergrammer",
  "author": "hobincar",
  "license": "MIT",
  "repository": "hobincar/react-clustergrammer",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
