    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "d3": "7.6.1",
    "jquery": "3.6.0",
    "react-fontawesome": "https://github.com/FortAwesome/react-fontawesome",
    "underscore": "1.13.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "files": [
    "dist"
  ]
}
